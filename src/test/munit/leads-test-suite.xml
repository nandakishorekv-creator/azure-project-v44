	xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" 
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="leads-test-suite.xml" />
	<munit:test name="post-leadsTest" doc:id="z2ntxxr7-m32g-xheu-hvk1-vcr0sewg109" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="b05uvavz-lr5t-pzfi-r9qg-8r6hd30xe96" processor="salesforce:create">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="salesforce-create-lead" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[{
    "id": null,
    "items": [
        {
            "exception": null,
            "message": null,
            "payload": {
                "success": true,
                "id": "001xx000003AAA",
                "errors": []
            },
            "id": "001xx000003AAA",
            "statusCode": null,
            "successful": true
        }
    ],
    "successful": true
}]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="post-leads" doc:id="ueye5pfx-20mv-b7kd-po23-cngmabpxg4q" name="post-leads"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="z5hb08ok-vy1f-cfuh-rt90-3eqbwk0cjjc" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success,
  message: payload.message,
  errors: payload.errors
}]'/>
		</munit:validation>
	</munit:test>
		<munit:test name="get-leadsTest" doc:id="y65zt0o1-pldl-1end-r5f3-yu8cldmb7fm" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="gi2mfcfj-fs1h-4ido-5bgh-546qnorbkfa" processor="salesforce:query">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="salesforce-query-leads" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[[
    {
        "Id": "001xx000003AAA",
        "Name": "Test Account",
        "Industry": "Technology"
    }
]]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="get-leads" doc:id="r3q1sknr-nr7e-sddp-bbpg-o2q6rcdmdxb" name="get-leads"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="0ikn55gq-hz1z-py2f-v8t4-ry325bw2ac" actual="#[payload]" expected='#[payload is Array]'/>
		</munit:validation>
	</munit:test>
		<munit:test name="put-leadsTest" doc:id="1caxtje1-y2z8-prnl-r9dl-40aym7r5jct" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="yv0wxts7-wi8t-sg4x-tm42-nz48kzwxbsd" processor="salesforce:update">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="salesforce-update-lead" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[[{"success": true, "id": "001xx000003AAA"}]]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="put-leads" doc:id="jikxz0uy-8rjy-07pv-bu3u-04rkmsl532jv" name="put-leads"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="i7ncjonx-30fd-0yv5-hc62-czzsb1f9kh" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success,
  message: payload.message,
  errors: payload.errors
}]'/>
		</munit:validation>
	</munit:test>
		<munit:test name="delete-leadsTest" doc:id="4mf0nv3e-s5bl-svfp-ib1a-hvqi7e4etjs" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="389u56ye-7j1t-htqx-nut7-2osn8qbq9nf" processor="salesforce:delete">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="salesforce-delete-lead" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[[{"success": true}]]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="delete-leads" doc:id="6ybxtqma-376k-q6u6-kyge-pnk3wtens" name="delete-leads"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="ntkkfzpc-oqii-0tpw-kq6w-pwue79drhob" actual="#[payload]" expected='#[{
  id: payload.id,
  success: payload.success,
  message: payload.message,
  errors: payload.errors
}]'/>
		</munit:validation>
	</munit:test>

</mule>